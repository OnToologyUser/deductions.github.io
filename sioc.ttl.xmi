    <UML:Namespace.ownedElement>
        <UML:Class 
            name="User Account"  xmi.id="http://rdfs.org/sioc/ns#UserAccount">
            <UML:Classifier.feature>
                <UML:Attribute name="first name" xmi.id="http://rdfs.org/sioc/ns#first_name">
                    <UML:StructuralFeature.type>
                        <UML:DataType xmi.idref="http://www.w3.org/2000/01/rdf-schema#Literal"/>
                    </UML:StructuralFeature.type>
                    <UML:ModelElement.taggedValue>
                        <UML:TaggedValue>
                            <UML:TaggedValue.dataValue xml:lang="en">First (real) name of this User. Synonyms include given name or christian name.</UML:TaggedValue.dataValue>
                        </UML:TaggedValue>
                    </UML:ModelElement.taggedValue>
                </UML:Attribute>
            </UML:Classifier.feature>
            <UML:ModelElement.taggedValue>
                <UML:TaggedValue>
                    <UML:TaggedValue.dataValue xml:lang="en">A user account in an online community site.</UML:TaggedValue.dataValue>
                </UML:TaggedValue>
            </UML:ModelElement.taggedValue>
            <UML:Classifier.feature>
                <UML:Attribute name="email sha1" xmi.id="http://rdfs.org/sioc/ns#email_sha1">
                    <UML:ModelElement.taggedValue>
                        <UML:TaggedValue>
                            <UML:TaggedValue.dataValue xml:lang="en">An electronic mail address of the UserAccount, encoded using SHA1.</UML:TaggedValue.dataValue>
                        </UML:TaggedValue>
                    </UML:ModelElement.taggedValue>
                    <UML:StructuralFeature.type>
                        <UML:DataType xmi.idref="http://www.w3.org/2000/01/rdf-schema#Literal"/>
                    </UML:StructuralFeature.type>
                </UML:Attribute>
            </UML:Classifier.feature>
            <UML:Classifier.feature>
                <UML:Attribute name="last name" xmi.id="http://rdfs.org/sioc/ns#last_name">
                    <UML:ModelElement.taggedValue>
                        <UML:TaggedValue>
                            <UML:TaggedValue.dataValue xml:lang="en">Last (real) name of this user. Synonyms include surname or family name.</UML:TaggedValue.dataValue>
                        </UML:TaggedValue>
                    </UML:ModelElement.taggedValue>
                    <UML:StructuralFeature.type>
