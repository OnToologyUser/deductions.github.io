    <UML:Namespace.ownedElement>
        <UML:Class 
            name="Item"  xmi.id="http://rdfs.org/sioc/ns#Item">
            <UML:Classifier.feature>
                <UML:Attribute name="content" xmi.id="http://rdfs.org/sioc/ns#content">
                    <UML:ModelElement.taggedValue>
                        <UML:TaggedValue>
                            <UML:TaggedValue.dataValue xml:lang="en">The content of the Item in plain text format.</UML:TaggedValue.dataValue>
                        </UML:TaggedValue>
                    </UML:ModelElement.taggedValue>
                    <UML:StructuralFeature.type>
                        <UML:DataType xmi.idref="http://www.w3.org/2000/01/rdf-schema#Literal"/>
                    </UML:StructuralFeature.type>
                </UML:Attribute>
            </UML:Classifier.feature>
            <UML:Classifier.feature>
                <UML:Attribute name="ip address" xmi.id="http://rdfs.org/sioc/ns#ip_address">
                    <UML:ModelElement.taggedValue>
                        <UML:TaggedValue>
                            <UML:TaggedValue.dataValue xml:lang="en">The IP address used when creating this Item. This can be associated with a creator. Some wiki articles list the IP addresses for the creator or modifiers when the usernames are absent.</UML:TaggedValue.dataValue>
                        </UML:TaggedValue>
                    </UML:ModelElement.taggedValue>
                    <UML:StructuralFeature.type>
                        <UML:DataType xmi.idref="http://www.w3.org/2000/01/rdf-schema#Literal"/>
                    </UML:StructuralFeature.type>
                </UML:Attribute>
            </UML:Classifier.feature>
            <UML:ModelElement.taggedValue>
                <UML:TaggedValue>
                    <UML:TaggedValue.dataValue xml:lang="en">An Item is something which can be in a Container.</UML:TaggedValue.dataValue>
                </UML:TaggedValue>
            </UML:ModelElement.taggedValue>
        </UML:Class>
